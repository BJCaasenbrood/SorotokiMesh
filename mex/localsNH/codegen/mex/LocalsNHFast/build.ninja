build $builddir/xgerc.o : cc $root/xgerc.c
build $builddir/xhseqr.o : cc $root/xhseqr.c
build $builddir/xdlanv2.o : cc $root/xdlanv2.c
build $builddir/xrot.o : cc $root/xrot.c
build $builddir/xzgehrd.o : cc $root/xzgehrd.c
build $builddir/xdlahqr.o : cc $root/xdlahqr.c
