# Link
build $root/LocalsNHFast_mex.mexa64 : link $builddir/LocalsNHFast_data.o $builddir/rt_nonfinite.o $builddir/LocalsNHFast_initialize.o $builddir/LocalsNHFast_terminate.o $builddir/LocalsNHFast.o $builddir/cross.o $builddir/eml_mtimes_helper.o $builddir/mtimes.o $builddir/mrdivide_helper.o $builddir/repmat.o $builddir/warning.o $builddir/qrsolve.o $builddir/det.o $builddir/indexShapeCheck.o $builddir/eye.o $builddir/eig.o $builddir/anyNonFinite.o $builddir/xnrm2.o $builddir/xdladiv.o $builddir/svd.o $builddir/xzsvdc.o $builddir/xdotc.o $builddir/_coder_LocalsNHFast_api.o $builddir/_coder_LocalsNHFast_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/xgeev.o $builddir/sumMatrixIncludeNaN.o $builddir/round.o $builddir/xzlarfg.o $builddir/xscal.o $builddir/xzlarf.o $builddir/xswap.o $builddir/xgemv.o $builddir/xgerc.o $builddir/xhseqr.o $builddir/xdlanv2.o $builddir/xrot.o $builddir/xzgehrd.o $builddir/xdlahqr.o $builddir/xdtrevc3.o $builddir/xaxpy.o $builddir/xzlascl.o $builddir/xzgebal.o $builddir/xzunghr.o $builddir/xdlaln2.o $builddir/unsafeSxfun.o $builddir/LocalsNHFast_emxutil.o $builddir/LocalsNHFast_mexutil.o $builddir/_coder_LocalsNHFast_info.o $builddir/c_mexapi_version.o 
